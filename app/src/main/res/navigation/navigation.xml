<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment">
    <fragment
        android:id="@+id/hitungFragment"
        android:name="org.d3if4081.ass1.ui.hitung.HitungFragment"
        android:label="Jajar Genjang"
        tools:layout="@layout/fragment_hitung" />
    <action
        android:id="@+id/action_hitungFragment_to_aboutFragment"
        app:destination="@id/aboutFragment" />
    <action
        android:id="@+id/action_hitungFragment_to_bendaJajarGenjangFragment"
        app:destination="@id/bendaJajarGenjangFragment" />
    <action
        android:id="@+id/action_hitungFragment_to_historiFragment"
        app:destination="@id/historiFragment" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.d3if4081.ass1.ui.AboutFragment"
        android:label="@string/tentang_aplikasi"
        tools:layout="@layout/fragment_about"/>
    <fragment
        android:id="@+id/bendaJajarGenjangFragment"
        android:name="org.d3if4081.ass1.ui.benda_jajar_genjang.BendaJajarGenjangFragment"
        android:label="@string/benda_jajar_genjang"
        tools:layout="@layout/fragment_benda_jajar_genjang"/>
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if4081.ass1.ui.histori.HistoriFragment"
        android:label="@string/histori"
        tools:layout="@layout/fragment_histori" />
</navigation>